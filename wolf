<script type="text/javascript">
  var imageURLs = [
       "http://i.imgur.com/GyPmaMh.jpg"
     , "http://i.imgur.com/b3NvEqf.jpg"
     , "http://i.imgur.com/kToMJn1.jpg"
     , "http://i.imgur.com/Cfstl1M.jpg"
     , "http://i.imgur.com/UmyC0uh.jpg"
     , "http://i.imgur.com/H5Gpdmj.jpg"
     , "http://i.imgur.com/ELUUvHA.jpg"
  ];
  function getImageTag() {
    var img = '<img src=\"';
    var randomIndex = Math.floor(Math.random() * imageURLs.length);
    img += imageURLs[randomIndex];
    img += '\" alt=\"Some alt text\"/>';
    return img;
  }
</script>
</head>
<body>
<script type="text/javascript">
  document.write(getImageTag());
</script>
</body>
